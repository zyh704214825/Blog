<script type="text/javascript">
	$(function(){
			$("#sub").click(function(){
				var tel=$("#tel").val();
				var cardid=$("#users_cardid").val()
				var str= /^1[34578]\d{9}$/;
				var str1=/^[1-9]\d{5}(18|19|([23]\d))\d{2}((0[1-9])|(10|11|12))(([0-2][1-9])|10|20|30|31)\d{3}[0-9Xx]$/;
				if($("#new_pswd").val()!=$("#renew_pswd").val()){
					$("#repswd").text("两次密码不一致");
					return false;
				}else{
					$("#repswd").text("");
				}
				if($("#new_pswd").val()==null||$("#new_pswd").val()==""){
					$("#pswd").text("请输入密码");
					return false;
				}else{
					$("#pswd").text("");
				}
				if($("#renew_pswd").val()==null||$("#renew_pswd").val()==""){
					$("#repswd").text("请重复输入密码");
					return false;
				}else{
					$("#repswd").text("");
				}
				return true;
			});
		});
